{
    "name": "livingyou",
    "version": "0.0.1",
    "type": "module",
    "main": "",
    "scripts": {
        "core-clean": "rimraf ./resource/core",
        "core-build": "npm run core-clean && swc ./src -d ./resources --copy-files",
        "core-dev": "npx nodemon --watch \"./src/**\" --ext \"ts,html,cfg,css,js\" --exec \"npm run core-build && altv-server.exe\"",
        "vue-build": "npx vite build ./src-webviews",
        "vue-serve": "npx vite preview ./src-webviews",
        "vue-dev": "npx vite ./src-webviews --clearScreen=false --debug=false --logLevel=silent",
        "dev": "npm-run-all --parallel core-dev vue-dev",
        "update": "npx altv-pkg d dev"
    },
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^2.1.7",
        "@altv/types-natives": "^1.4.4",
        "@altv/types-server": "^2.3.5",
        "@altv/types-shared": "^1.2.4",
        "@altv/types-webview": "^1.0.2",
        "@altv/types-worker": "^1.0.6",
        "@babel/types": "^7.17.10",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.177",
        "@types/node": "^17.0.31",
        "@types/node-fetch": "^2.6.1",
        "@vitejs/plugin-vue": "^2.3.2",
        "altv-pkg": "^2.0.3",
        "nodemon": "^2.0.16",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "typescript": "4.6.4",
        "vite": "^2.9.8",
        "vue": "^3.2.33",
        "vue-resize-text": "^0.1.1",
        "vue-tsc": "^0.34.11"
    },
    "dependencies": {
        "@stuyk/ezmongodb": "^2.0.2",
        "dotenv": "^16.0.0",
        "node-fetch": "^3.2.4"
    }
}
