{
    "name": "livingyou",
    "version": "0.0.1",
    "type": "module",
    "main": "",
    "scripts": {
        "clean-core": "rimraf ./resource/core",
        "build": "npm run clean-core && swc ./src -d ./resources --copy-files",
        "update": "altv-pkg d dev",
        "livingyou-dev": "npx nodemon --watch \"./src/**\" --ext \"ts,html,cfg,css,js\" --exec \"npm run build && altv-server.exe\"",
        "vue-dev": "npx vite ./src-webviews --clearScreen=false --debug=false --logLevel=silent",
        "dev": "npm-run-all --parallel livingyou-dev vue-dev"
    },
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^2.1.4",
        "@altv/types-natives": "^1.4.4",
        "@altv/types-server": "^2.3.1",
        "@altv/types-shared": "^1.2.3",
        "@altv/types-webview": "^1.0.2",
        "@altv/types-worker": "^1.0.6",
        "@babel/types": "^7.17.0",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.165",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.24",
        "@types/node-fetch": "^2.6.1",
        "@types/sjcl": "^1.0.30",
        "@vitejs/plugin-vue": "^2.3.1",
        "altv-pkg": "^1.2.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "typescript": "4.6.3",
        "vite": "^2.9.4",
        "vue": "^3.2.32",
        "vue-resize-text": "^0.1.1",
        "vue-tsc": "^0.34.6"
    },
    "dependencies": {
        "@stuyk/ezmongodb": "^2.0.1",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "node-fetch": "^3.2.3",
        "sjcl": "^1.0.8"
    }
}
