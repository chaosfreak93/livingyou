{
    "name": "livingyou",
    "version": "0.0.1",
    "type": "module",
    "main": "",
    "scripts": {
        "clean": "rimraf ./resources",
        "build": "npm run clean && swc ./src -d ./resources --copy-files",
        "update": "altv-pkg d dev",
        "windows": "npm run build && altv-server.exe",
        "linux": "npm run build && ./start.sh",
        "windev": "nodemon --watch \"./src/**\" --ext \"ts,html,cfg,css,js\" --exec \"npm run build && altv-server.exe\"",
        "dev": "npm run windev"
    },
    "author": "stuyk",
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^2.1.3",
        "@altv/types-natives": "^1.4.4",
        "@altv/types-server": "^2.3.0",
        "@altv/types-shared": "^1.2.3",
        "@altv/types-webview": "^1.0.2",
        "@altv/types-worker": "^1.0.6",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.165",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.23",
        "@types/node-fetch": "^2.6.1",
        "@types/sjcl": "^1.0.30",
        "altv-pkg": "^1.2.0",
        "nodemon": "^2.0.15",
        "rimraf": "^3.0.2",
        "tsup": "^4.14.0",
        "typescript": "4.6.3"
    },
    "dependencies": {
        "@stuyk/ezmongodb": "^2.0.1",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "node-fetch": "^3.2.3",
        "sjcl": "^1.0.8"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": true,
        "semi": true,
        "arrowParens": "always"
    }
}
